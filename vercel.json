{
    "version": 2,
    "projects": [
      {
        "name": "client",
        "path": "./client",
        "routes": [
          {
            "src": "/(.*)",
            "dest": "client/$1"
          }
        ],
        "builds": [
          {
            "src": "client/package.json",
            "use": "@vercel/next"
          }
        ]
      },
      {
        "name": "server",
        "path": "./server",
        "routes": [
          {
            "src": "/api/(.*)",
            "dest": "server/api/$1"
          }
        ],
        "builds": [
          {
            "src": "server/server.js",
            "use": "@vercel/node"
          }
        ]
      }
    ]
  }